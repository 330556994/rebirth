{
  "name": "rebirth",
  "version": "0.0.1",
  "description": "Angular2 blog",
  "author": [],
  "homepage": "http://git.oschina.net/greengerong/rebirth",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "run-parallel": "npm-run-all --parallel",
    "ng-jit": "ng serve",
    "ng-aot": "ng serve  --aot --prod",
    "mock": "node ./build/mock-server.js",
    "jit": "npm run run-parallel mock ng-jit",
    "aot": "npm run run-parallel mock ng-aot",
    "pwa": "node ./build/sw-precache.js",
    "github": "ng serve --aot --env=github",
    "github-pages:push": "ngh --message \"rebirth blog deploy\"",
    "github:deploy": "ng build --aot --prod --env=github --base-href=/rebirth/ && npm run pwa && npm run github-pages:push",
    "lint": "tslint \"src/**/*.ts\"",
    "lint-fix": "tslint -c tslint.json \"src/**/*.ts\"  --fix",
    "test": "karma start ./karma.conf.js",
    "protractor": "protractor",
    "typedoc": "typedoc",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "build": "ng build --prod",
    "start": "ng serve",
    "prepree2e": "npm start",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e": "protractor ./protractor.conf.js",
    "ssr": "npm run build:ssr && npm run run-parallel mock run:ssr",
    "run:ssr": "nodemon ./dist-server/server.bundle.js",
    "build:ssr": "npm run run-parallel build:ssr:client build:ssr:server",
    "build:ssr:client": "ng build --aot",
    "build:ssr:server": "webpack --config webpack-server.config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "4.2.0-beta.0",
